{
  "name": "reveng-web-interface",
  "version": "1.0.0",
  "description": "Web interface for AI-Enhanced Universal Binary Analysis Engine",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/server.js",
    "install-all": "npm install && cd client && npm install",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "node-cron": "^3.0.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "reverse-engineering",
    "binary-analysis",
    "security",
    "ai",
    "web-interface"
  ],
